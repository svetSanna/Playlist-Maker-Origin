<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="@dimen/margin_312"
                android:height="@dimen/margin_312"/>
            <stroke
                android:width="@dimen/margin_1"
                android:color="@color/image_newplaylist_background"
                android:dashWidth="@dimen/margin_24"
                android:dashGap="@dimen/margin_24"/>
            <corners android:radius="@dimen/margin_8"/>
        </shape>
    </item>
    <item
        android:width="@dimen/margin_100"
        android:height="@dimen/margin_100"
        android:gravity="center"
        android:drawable="@drawable/ic_add_image_playlist">
    </item>
</layer-list>